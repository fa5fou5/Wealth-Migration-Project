    <DTS:Executable
      DTS:refId="Package\Second hal of the month loop"
      DTS:AssignExpression="@LoopCounter2 = @LoopCounter2 + 1"
      DTS:CreationName="STOCK:FORLOOP"
      DTS:Description="Conteneur de boucles For"
      DTS:DTSID="{e40b66c4-ecf9-476a-a945-e7987d1f39b8}"
      DTS:EvalExpression="@LoopCounter2 &lt;= 2"
      DTS:ExecutableType="STOCK:FORLOOP"
