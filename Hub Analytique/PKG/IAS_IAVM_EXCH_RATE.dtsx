          <ProjectItem
            Name="ScriptMain.cs"
            Encoding="UTF8"><![CDATA[using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.IO;

namespace ST_6850f2822fc44f6ba6d14bd68b12a07e
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{

		public void Main()
		{
            string path = (string)Dts.Variables["User::controlFilePath"].Value;
            DateTime lastUpdateDate = (DateTime)Dts.Variables["User::lastUpdateDate"].Value;
            int nbRows = (int)Dts.Variables["User::nbRows"].Value;

            Dts.TaskResult = (int)WriteFile(path, lastUpdateDate, nbRows);            
		}

        private DTSExecResult WriteFile(string path,DateTime currentDate,int nbRows)
        {
            string header = "LastUpdate;NbRows";
            string colone = currentDate.ToString("yyyy-MM-dd HH:mm:ss") + ";" + nbRows.ToString();
            StreamWriter writer = null;

            try
            {
                writer = new StreamWriter(path, false);
                writer.WriteLine(header);
                writer.WriteLine(colone);
                writer.Close();

                return (int)DTSExecResult.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "IAS_IAVM_EXCH_RATE - Write control file", ex.Message + "\r\n" + ex.StackTrace, null, 0);
            }
            finally
            {
                if (writer != null)
                {
                    writer.Dispose();
                }
            }

            return DTSExecResult.Failure;
        }

	}
}]]></ProjectItem>
