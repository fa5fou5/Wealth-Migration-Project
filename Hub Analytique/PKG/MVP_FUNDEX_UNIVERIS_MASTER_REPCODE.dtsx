          <ProjectItem
            Name="ScriptMain.cs"
            Encoding="UTF8"><![CDATA[using System;
using Microsoft.SqlServer.Dts.Runtime;
using System.IO;

namespace ST_be48938f3e4540d9920966d865510067
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
        private const string ComponentName = "FUNDEX_UNIVERIS_MASTER_REPCODE - Script task Validations";

		public void Main()
		{
            var currentDate = (DateTime)Dts.Variables["currentDate"].Value;
            var controlFilePath = (string)Dts.Variables["workingFolder"].Value + "\\" + (string)Dts.Variables["controlFileName"].Value;

            if (ISAnyControlFile(controlFilePath) == DTSExecResult.Success)
            {
                if (IsNewUpdateRequired(currentDate, controlFilePath) == DTSExecResult.Success)
                {
                    Dts.TaskResult = (int)DTSExecResult.Success;
                    return;
                }
            }
            else
            {
                Dts.Events.FireWarning(0, ComponentName, "Control file not found", null, 0);
                Dts.TaskResult = (int)DTSExecResult.Success;
                return;
            }

            Dts.TaskResult = (int)DTSExecResult.Completion;
		}

        private DTSExecResult ISAnyControlFile(string controlFilePath)
        {
            if (File.Exists(controlFilePath) == true)
            {
                return DTSExecResult.Success;
            }

            return DTSExecResult.Failure;
        }
        private DTSExecResult IsNewUpdateRequired(DateTime currentDate,string controlFilePath)
        {
            StreamReader reader = null;

            try
            {
                reader = new StreamReader(controlFilePath);
                reader.ReadLine();//read the header
                var values = reader.ReadLine().Split(';');//read the colones

                reader.Close();

                var lastUpdate = DateTime.Parse(values[0]);

                if(lastUpdate.Year == currentDate.Year)
                {
                    if(lastUpdate.Month == currentDate.Month)
                    {
                        if (lastUpdate.Day == currentDate.Day)
                        {
                            return DTSExecResult.Failure;//validations failed
                        }
                    }
                }

                return DTSExecResult.Success;
            }
            catch(Exception ex)
            {
                Dts.Events.FireError(0, ComponentName, ex.Message + "\r\n" + ex.StackTrace, null, 0);
            }
            finally
            {
                if(reader != null)
                {
                    reader.Dispose();
                }
            }

            return DTSExecResult.Failure;
        }
    }
}]]></ProjectItem>
