          <ProjectItem
            Name="ScriptMain.cs"
            Encoding="UTF8"><![CDATA[using System;
using Microsoft.SqlServer.Dts.Runtime;
using System.IO;

namespace ST_6850f2822fc44f6ba6d14bd68b12a07e
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{

		public void Main()
		{
            string path = (string)Dts.Variables["User::workingFolder"].Value + "\\" + (string)Dts.Variables["User::controlFileName"].Value;
            DateTime currentDate = (DateTime)Dts.Variables["User::currentDate"].Value;

            var resultFile = (string)Dts.Variables["User::resultFileName"].Value + "_" +
                             (string)Dts.Variables["User::dataDescription"].Value + "_" + 
                             (string)Dts.Variables["User::resultFileName"].Value;

            Dts.TaskResult = (int)WriteFile(path, currentDate, resultFile);            
		}

        private DTSExecResult WriteFile(string path,DateTime currentDate, string resultFile)
        {
            string header = "LastUpdates";
            string colone = currentDate.Date.ToString();
            StreamWriter writer = null;

            try
            {
                writer = new StreamWriter(path, false);
                writer.WriteLine(header);
                writer.WriteLine(colone);
                writer.Close();

                return (int)DTSExecResult.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, resultFile + " - Write control file", ex.Message + "\r\n" + ex.StackTrace, null, 0);
            }
            finally
            {
                if (writer != null)
                {
                    writer.Dispose();
                }
            }

            return DTSExecResult.Failure;
        }

	}
}]]></ProjectItem>
