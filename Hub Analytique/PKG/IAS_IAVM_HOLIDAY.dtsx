          <ProjectItem
            Name="ScriptMain.cs"
            Encoding="UTF8"><![CDATA[using System;
using MySql.Data.MySqlClient;
using Microsoft.SqlServer.Dts.Runtime;
using System.IO;

namespace ST_be48938f3e4540d9920966d865510067
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
        private const string ComponentName = "IAS_IAVM_HOLIDAY - Script task Validations";

		public void Main()
		{
            // last update date default to yesterday as data is current date
            var lastUpdateDate = DateTime.Today.AddDays(-2);    
            var controlFilePath = (string)Dts.Variables["controlFilePath"].Value;

            if (ISAnyControlFile(controlFilePath) == DTSExecResult.Success)
            {
                DateTime? controlDate = ReadControlFileDate(controlFilePath);
                if (controlDate.HasValue)
                {
                    lastUpdateDate = controlDate.Value;
                }
                else
                {
                    Dts.Events.FireWarning(0, ComponentName, "Control file contains no date", null, 0);
                }
            }
            else
            {
                Dts.Events.FireWarning(0, ComponentName, "Control file not found", null, 0);
            }

            // write the last update date to the package variable (to be used in the query)
            Dts.Variables["lastUpdateDate"].Value = lastUpdateDate;
            Dts.TaskResult = (int)DTSExecResult.Success;
            return;
		}

        private DTSExecResult IsAnyDataAvailable(DateTime currentDate)
        {
            var query = $"SELECT COUNT(*) AS 'nbRows' FROM IAVM.holiday WHERE TIMESTAMP(current_date) > DATE_FORMAT('{currentDate.ToString("yyyy-MM-dd HH:mm:ss")}', '%Y-%m-%d %H:%i:%s')";
            MySqlConnection db = null;

            try
            {
                db = Dts.Connections["IAVM"].AcquireConnection(null) as MySqlConnection;
 
                MySqlCommand cmd = new MySqlCommand(query, db);
                var nbRows = (Int64)cmd.ExecuteScalar();

                if (nbRows > 0)
                {
                    return DTSExecResult.Success;
                }
            }
            catch(Exception ex)
            {
                Dts.Events.FireError(0, ComponentName, ex.Message + "\r" + ex.StackTrace, String.Empty, 0);
            }
            finally
            {
                if (db != null)
                {
                    Dts.Connections["IAVM"].ReleaseConnection(db);
                }
            }

            return DTSExecResult.Failure; 
        }
        private DTSExecResult ISAnyControlFile(string controlFilePath)
        {

            if (File.Exists(controlFilePath) == true)
            {
                return DTSExecResult.Success;
            }

            return DTSExecResult.Failure;
        }
        private DTSExecResult IsNewUpdateRequired(DateTime currentDate,string controlFilePath)
        {
            StreamReader reader = null;

            try
            {
                reader = new StreamReader(controlFilePath);
                reader.ReadLine();//read the header
                var values = reader.ReadLine().Split(';');//read the colones

                reader.Close();

                var lastUpdate = DateTime.Parse(values[0]);

                if(lastUpdate.Year == currentDate.Year)
                {
                    if(lastUpdate.Month == currentDate.Month)
                    {
                        return DTSExecResult.Failure;//validations failed
                    }
                }

                return DTSExecResult.Success;
            }
            catch(Exception ex)
            {
                Dts.Events.FireError(0, ComponentName, ex.Message + "\r\n" + ex.StackTrace, null, 0);
            }
            finally
            {
                if(reader != null)
                {
                    reader.Dispose();
                }
            }

            return DTSExecResult.Failure;
        }

        private DateTime? ReadControlFileDate(string controlFilePath)
        {
            StreamReader reader = null;

            try
            {
                reader = new StreamReader(controlFilePath);
                reader.ReadLine();  //read the header & discard
                var values = reader.ReadLine().Split(';');  //read the columns
                reader.Close();

                return DateTime.Parse(values[0]);
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, ComponentName, ex.Message + "\r\n" + ex.StackTrace, null, 0);
            }
            finally
            {
                if (reader != null)
                {
                    reader.Dispose();
                }
            }

            return null;
        }
    }
}]]></ProjectItem>
